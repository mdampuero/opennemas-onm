<VirtualHost {IP}:80>

	ServerName      {SITE_DOMAINS}
	DocumentRoot    {SITE_PATH}
	
	ErrorLog        {TMP_PATH}/{ID}_error.log
	CustomLog       {TMP_PATH}/{ID}_access.log combined
	DirectoryIndex  index.html index.htm index.php index.php4 index.php5
	
	<Directory {SITE_PATH}>
        Options -Indexes IncludesNOEXEC FollowSymLinks
        Allow from all
        AllowOverride All
	</Directory>

</VirtualHost>