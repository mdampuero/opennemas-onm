<VirtualHost *:80>

	ServerAdmin fran.dieguez@mabishu.com
	ServerName onm.branch
	ServerAlias onm.local

	DocumentRoot /home/fran/Projects/OpenHost/onm-v8/public
	<Directory /home/fran/Projects/OpenHost/onm-v8/public>
		Options Indexes FollowSymLinks MultiViews
		AllowOverride All
		Order allow,deny
		allow from all
	</Directory>

	LogLevel warn

	ErrorLog /home/fran/Projects/OpenHost/onm-v8/tmp/logs/apache-errors.log
	CustomLog /home/fran/Projects/OpenHost/onm-v8/tmp/logs/apache-access.log combined
	RewriteLog /home/fran/Projects/OpenHost/onm-v8/tmp/logs/apache-access.log

	ServerSignature On

</VirtualHost>
