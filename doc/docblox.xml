<?xml version="1.0" encoding="UTF-8" ?>
<docblox>
    <parser>
        <target>doc/docblox</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
            <extension>php3</extension>
            <extension>phtml</extension>
        </extensions>
        <visibility></visibility>
