            <errors>{APP_ROOT}/doc/docblox/log/{DATE}.errors.log</errors>
        </paths>
    </logging><!--
    <transformations>
        <template name="old_ocean" />
    </transformations> -->
    <files>
        <directory>public/core</directory>
        <directory>app/models</directory>
        <directory>vendor/Panorama/</directory>
        <directory>vendor/Onm/</directory>
    </files>
</docblox>
