{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
  {% set icon %}
    <img width="30" height="15" title="" alt="" style="width: 30px; height: 15px; max-width: 30px; max-height: 15px; margin-top: 5px; margin-bottom: -3px; display: inline;" src="data:image/png;base64," />
  {% endset %}

  {% set text %}
    <div class="sf-toolbar-info-group">
      <div class="sf-toolbar-info-piece">
        <b>CMS</b>
        <span>opennemas</span>
      </div>
      <div class="sf-toolbar-info-piece">
        <b>Web</b>
        <span>
          <a href="{{ collector.data.homepage }}" target="_blank">
            {{ collector.data.homepage | replace({ 'https://': '' }) }}
          </a>
        </span>
      </div>
      <div class="sf-toolbar-info-piece">
        <b>Version</b>
        <span>{{ collector.data.version }}</span>
      </div>
    </div>
  {% endset %}

  {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}
