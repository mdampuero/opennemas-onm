<body>

	<div id="site_container">
		<div id="site_header">
			<ul class="corporate_identity {if $enterprisey}enterprise_edition{/if}">
				<li class="logo"><a href="http://www.opennemas.com"><span>Opennemas Framework</span></a></li>
			</ul>
		</div>
		<div id="site_body">
            
            {if $smarty.const.APPLICATION_ENV == 'development'}
            
            <h1 class="error_title">{$message}</h1>
            <div id="content">
                    
                {if $app_name}{$app_name}{else}OpenNemas Framework{/if} encountered the following error:
                
                <p>
                    {$exception->getMessage()}
                </p>
                
                <dl>
                <dt>Exception class:</dt>
                <dd>
                    <a href="kmd://{$exception->getFile()}#{$exception->getLine()}">{$exception->getFile()}</a>
                    at line {$exception->getLine()}
                </dd>
                
                <dt>Backtrace:</dt>
                <dd>
                    <table class="backtrace">
                        {*$exception->getTrace()|var_dump*}
                        <tr>
                            <th>File</th>
                            <th>Line</th>
                            <th>Class</th>
                            <th>Function</th>
                            {*<th>Args</th>*}
                        </tr>
                        {foreach $exception->getTrace() as $trace}
                        <tr class="backtrace_line framework {cycle values="uneven,even"}">
                           <td><a href="kmd://{$trace['file']}#{$trace['line']}">{$trace['file']}</a></td>
                           <td>{$trace['line']}</td>
                           <td>{$trace['class']}</td>
                           <td>{$trace['function']}</td>
                           {*<td>{$trace['args']|var_dump}</td>*}
                        </tr>
                        {/foreach}

                    </table>   
                </dd>
                </dl>
                
                <dt>Request Parameters:</dt>
                <dd>
                    <table class="backtrace">
                        {*$exception->getTrace()|var_dump*}
                        <tr>
                            <th>Name</th>
                            <th>Value</th>
                        </tr>
                        {foreach $request->getParams() as $param}
                        <tr class="backtrace_line framework {cycle values="uneven,even"}">
                           <td>{$param@key}</td>
                           <td>{$param}</td>
                        </tr>
                        {/foreach}

                    </table>
                </dd>
        
            </div>
            {else}
            <div id="content">
                There was an error with the application. Please contact with your developer.
            </div>
            {/if}

		</div>
		<div id="site_footer">
			<!--
			   You are free to modify the footer as you see fit,
			   but we kindly ask of you to preserve to following
			   text. Thank you.
			-->
			<div class="container">
				Powered by <a href="http://www.opennemas.com/">Opennemas Framework</a>{if $enterprisey}(Enterprise Edition){/if}
			</div>
		</div>
	</div>

