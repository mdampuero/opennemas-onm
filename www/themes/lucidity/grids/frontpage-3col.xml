<?xml version="1.0"?>
<grid>
    <name>Frontpage default</name>
    <description></description>
    
    <content><![CDATA[<div class="span-24" role="wairole:grid">    
    <div class="layout-column last-column last span-8">
        <div id="content-left" role="wairole:gridcell"><!-- #content-left# --></div>
    </div>
    <div class="layout-column last-column last span-8">
        <div class="border-dotted" id="content-center" role="wairole:gridcell"><!-- #content-center# --></div>
    </div>
    <div class="layout-column last-column last span-8">
        <div class="border-dotted" id="content-right" role="wairole:gridcell"><!-- #content-right# --></div>
    </div>
    
    <hr class="space" />
    
    <div class="layout-column last-column last span-24">
        <div id="content-long" role="wairole:gridcell"><!-- #content-long# --></div>
    </div>
</div>]]></content>
    
    <positions>        
        <position>content-left</position>
		<position>content-center</position>
		<position>content-right</position>
        <position>content-long</position>
	</positions>    
    
</grid>
