{
    "name": "openhost/opennemas",
    "description": "High performance and scalable NMS focused in journalism workflow",
    "keywords": ["nms", "newspapers", "magazines", "content management system"],
    "homepage": "http://opennemas.com",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "OpenHost developers",
            "email": "developers@openhost.es"
        }
    ],
    "include-path": [
        "libs/",
        "libs/core",
        "libs/models/",
        "src/",
        "src/WebService/Handlers/"
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ],
        "files": [
            "libs/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "doctrine/doctrine-bundle": "~1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "incenteev/composer-parameter-handler": "~2.0",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/monolog-bundle": "~3.0.2",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "~2.3.10",
        "symfony/symfony": "3.3.*",
        "twig/twig": "^1.0||^2.0",

        "drak/native-session": "*",
        "econea/nusoap": "0.9.*",
        "friendsofsymfony/jsrouting-bundle": "2.2.*",
        "giggsey/libphonenumber-for-php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "hwi/oauth-bundle": "^0.6.3",
        "imagine/imagine": "0.7.*",
        "jagandecapri/symfony-parallel-process": "*",
        "jdorn/sql-formatter": "1.2.16",
        "jeremykendall/php-domain-parser": "~2.0",
        "league/csv": "~8.0",
        "lexik/jwt-authentication-bundle": "dev-master",
        "luracast/restler": "3.0.0",
        "php-http/guzzle6-adapter": "1.1.*",
        "php-http/httplug-bundle": "1.11.*",
        "ronanguilloux/isocodes": "^1.2",
        "smarty-gettext/smarty-gettext": "@stable",
        "smarty/smarty" : "~3.1",
        "symfony/assetic-bundle": "~2.2",

        "drexarj/disqus-php": "dev-master",
        "cometcult/braintree-bundle": "dev-master",
        "frandieguez/panorama-php": "1.0.*",
        "google/recaptcha": "1.1.3",
        "paypal/adaptivepayments-sdk-php": "v3.9.2",
        "paypal/merchant-sdk-php": "v3.9.1",
        "rtconner/freshbooks-api": "dev-master"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "*",

        "mockery/mockery": "dev-master",
        "pdepend/pdepend": "2.2.*",
        "phploc/phploc": "3.0.*",
        "phpmd/phpmd": "2.3.*",
        "phpunit/phpunit": "6.*",
        "sebastian/phpcpd": "2.0.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/frandieguez/jwt-authentication-bundle"
        },
        {
            "type": "git",
            "url": "https://github.com/zikula/NativeSession"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "tmp",
        "symfony-web-dir": "public",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
