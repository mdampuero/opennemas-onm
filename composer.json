{
    "name": "openhost/opennemas",
    "description": "High performance and scalable NMS focused in journalism workflow",
    "keywords": ["nms", "newspapers", "magazines", "content management system"],
    "homepage": "http://opennemas.com",
    "type": "cms",
    "license": "proprietary",
    "version": "1.2",
    "authors": [
        {
            "name": "OpenHost developers",
            "email": "developers@openhost.es"
        }
    ],
    "include-path": [
        "app/",
        "app/models/",
        "libs/",
        "libs/core",
        "src/",
        "src/WebService/Handlers/",
        "vendor/"
    ],
    "autoload": {
        "psr-4": {
            "": "src/",
            "SymfonyStandard\\": "app/SymfonyStandard/"
        },
        "files": [
            "libs/functions.php",
            "libs/adodb5/adodb.inc.php",
            "libs/Pager/Pager.php"
        ]
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "2.3.*",
        "sebastian/phpcpd": "2.0.*",
        "pdepend/pdepend": "2.2.*",
        "phploc/phploc": "2.1.*",
        "phpunit/phpunit": "4.8.*",
        "theseer/phpdox": "0.8.*"
    },

    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.7",
        "symfony/monolog-bundle": "~2.4",
        "symfony/swiftmailer-bundle": "~2.3",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "cometcult/braintree-bundle": "dev-master",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "doctrine/migrations": "dev-master",
        "drak/native-session": "*",
        "drexarj/disqus-php": "dev-master",
        "frandieguez/panorama-php": "1.0.*",
        "friendsofsymfony/jsrouting-bundle": "1.5.*",
        "imagine/imagine": "0.6.*",
        "luracast/restler": "3.0.0",
        "mockery/mockery": "dev-master",
        "paypal/adaptivepayments-sdk-php": "v3.8.107",
        "paypal/merchant-sdk-php": "v3.8.107",
        "php-ffmpeg/php-ffmpeg": "0.5.*@dev",
        "hwi/oauth-bundle": "dev-master",
        "recaptcha/php5": "1.1.0",
        "rtconner/freshbooks-api": "dev-master",
        "sparkling/vat-bundle": "~0.1",
        "google/recaptcha": "1.1",
        "smarty/smarty" : "3.1.21",
        "smarty-gettext/smarty-gettext": "@stable",
        "lexik/jwt-authentication-bundle": "dev-master",
        "league/csv": "~7.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/openhost/jwt-authentication-bundle"
        },
        {
            "type": "git",
            "url": "https://github.com/zikula/NativeSession"
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "public",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
